<h1>Games</h1>
<p>Find me in app/views/games/index.html.erb</p>




<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Platform</th>
      <th>Year</th>
      <th>Genre</th>
      <th>Publisher</th>
      <th>Developer</th>
      <th>Rating</th>
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><%= game.name %></td>
        <td><%= game.platform.name %></td>
        <td><%= game.year %></td>
        <td><%= game.genre.name %></td>
        <td><%= game.publisher %></td>
        <td><%= game.developer %></td>
        <td><%= game.rating %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @game%>
